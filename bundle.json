{
  "name": "snmp",
  "version": "1.0",
  "author": "Zack YL Shih",
  "email": "ZackYL.Shih@moxa.com",
  "description": "This is an openvpn model",
  "license": "MOXA",
  "main": "openvpn.py",
  "argument": "",
  "priority": 82,
  "retry": 200,
  "retryInterval": 500,
  "hook": [],
  "dependencies": {},
  "repository": {},
  "resources": [
    {
      "uri": "/network/openvpn/files/",
      "downloadFile": {
        "path": "/home/model/network/openvpn/config/files/",
        "name": "export.tar"
      }
    },
    {
      "methods": ["put", "get"],
      "uri": "/network/openvpn",
      "uploadFile": {
        "allowedFiles": [
          "static.key",
          "clientCa.crt",
          "clientPublic.crt",
          "clientPrivate.key",
          "tlsAuth.key",
          "client.opvn"
        ],
        "path": "/home/model/network/openvpn/config/files/"
      }
    }
  ],
  "view": {
    "title": "Test",
    "resource": "http://mar2000.apiary-mock.com/api/loadBalance",
    "externalResources": [
      {
        "url": "/api/loadBanlance",
        "as": "lb"
      }
    ],
    "info": [
      {
        "component": "list",
        "items": [
          {
            "label": "Read Community",
            "ngModel": "readCommunity"
          },
          {
            "label": "Write Community",
            "ngModel": "writeCommunity"
          },
          {
            "label": "Port",
            "ngModel": "port"
          }
        ]
      },
      {
        "component": "map",
        "ngModel": "map"
      }
    ],
    "edit": {
      "form": {
        "action": "http://mar2000.apiary-mock.com/api/loadBalance",
        "method": "put",
        "fields": [
          {
            "id": "enable",
            "component": "checkbox",
            "disabled": false,
            "label": "Enable",
            "ngModel": "enable",
            "options": [
              "enable"
            ]
          },
          {
            "id": "readCommunity",
            "component": "textInput",
            "disabled": false,
            "label": "Read Community",
            "ngModel": "readCommunity",
            "validation": "required"
          },
          {
            "id": "writeCommunity",
            "component": "textInput",
            "disabled": false,
            "label": "Write Comminity",
            "ngModel": "writeCommunity",
            "validation": "required"
          },
          {
            "id": "port",
            "component": "textInput",
            "disabled": false,
            "label": "Port",
            "ngModel": "port",
            "validation": ["required", "number"]
          }
        ]
      }
    }
  }
}
